    "# альтернатива itertools.cycle\n",
    "\n",
    "def my_cycle(iterable):\n",
    "    saved = []\n",
    "    for element in iterable:\n",
    "        yield element\n",
    "\n",
    "n = 0\n",
    "for i in my_cycle(\"ABCD\"):\n",
    "    if n >= 30:\n",
    "        break\n",
    "    print(i, end=' ')\n",
    "    n += 1"
